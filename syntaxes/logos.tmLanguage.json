{
	"fileTypes": ["xm", "x", "xi", "xmi"],
	"name": "Logos",
	"patterns": [
		{
			"match": "%(init|hook|subclass|group|class|new|ctor|dtor|end|config|orig|log)",
			"name": "keyword.source.logos"
		},
		{
			"captures": {
				"1": {
					"name": "keyword.source.logos"
				}
			},
			"match": "%c\\(([A-Za-z$_]+)\\)"
		},
		{
			"include": "source.objc"
		},
		{
			"include": "source.c++"
		}
	],
	"scopeName": "source.logos"
}
